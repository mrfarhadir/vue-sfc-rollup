{
  "name": "<%-npmName%>",
  "version": "1.0.0",
  "description": "",

  "main": "dist/<%-componentName%>.ssr.js",
  "browser": "dist/<%-componentName%>.esm.js",
  "module": "dist/<%-componentName%>.esm.js",
  "unpkg": "dist/<%-componentName%>.min.js",
<% if (ts) { -%>
  "types": "<%-componentName%>.d.ts",
<% } -%>

  "files": [
    "dist/*",
<% if (ts) { -%>
    "<%-componentName%>.d.ts",
<% } -%>
    "src/**/*.vue"
  ],
  "sideEffects": false,

  "scripts": {
    "serve": "vue-cli-service serve dev/serve.<% if (ts) { %>ts<% } else { %>js<% } %>",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },

  "dependencies": {
  },

  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
<% if (ts) { -%>
    "@babel/preset-typescript": "^7.13.0",
<% } -%>
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.12",
<% if (ts) { -%>
    "@vue/cli-plugin-typescript": "^4.5.12",
<% } -%>
    "@vue/cli-service": "^4.5.12",
<% if (version === 3) { -%>
    "@vue/compiler-sfc": "^3.0.11",
<% } -%>
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
<% if (version === 3) { -%>
    "postcss": "^8.2.10",
<% } -%>
    "rollup": "^2.45.2",
<% if (version === 3) { -%>
    "rollup-plugin-postcss": "^4.0.0",
<% } -%>
    "rollup-plugin-terser": "^7.0.2",
<% if (version === 3) { -%>
    "rollup-plugin-vue": "^6.0.0",
<% } else { -%>
    "rollup-plugin-vue": "^5.1.9",
<% } -%>
<% if (ts) { -%>
    "typescript": "^4.2.4",
<% } -%>
<% if (version === 3) { -%>
    "vue": "^3.0.5"
<% } else { -%>
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
<% } -%>
  },
  "peerDependencies": {
<% if (version === 3) { -%>
    "vue": "^3.0.5"
<% } else { -%>
    "vue": "^2.6.12"
<% } -%>
  },
  "engines": {
    "node": ">=12"
  }
}
